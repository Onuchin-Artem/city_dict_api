* { margin: 0; padding: 0; outline: 0; }
article, aside, details, footer, header, menu, nav, section { display: block; }
body {
	font-size: 16px;
	line-height: 22px;
	font-family: arial, sans-serif;
	color: #828282;
	background: #FFFFFF repeat 0 0;
}
a { color: #067aa7; text-decoration: none; cursor: pointer;font-family: arial, sans-serif; }
a:hover { text-decoration: underline; }
a img { border: 0; }
p {margin: 0 0 10px}

.shell {width:90%; margin: 0 auto; min-width: 900px; padding: 0 30px;position: relative}
.main { padding:0px; position: relative; height: 100%;}
.main h1 { font-size: 36px; color: #2d2d2d; padding-top: 20px; line-height: 36px; font-family: Arial, sans-serif; font-weight: 500; padding-bottom: 12px; }
.main h2 { font-size: 28px; color: #2d2d2d; padding-top: 10px; line-height: 28px; font-family: Arial, sans-serif; font-weight: 500; padding-bottom: 12px; }
.main h3 { font-size: 22px; color: #2d2d2d; line-height: 26px; font-family: Arial, sans-serif; font-weight: 500; padding-bottom: 8px; }
.main section ul {
    list-style: none;
}
.main section ul li {
    height: 40px;
    padding-left: 50px;
    line-height: 19px;
    padding-bottom: 8px;
}
.main section ul li a{
    display: inline-block;
    font-size: 14px;
}
.main section ul li.opensreet {
    background: url(<%= asset_path "openstreetmap.png" %>) no-repeat 0 0;
}
.main section ul li.dbpedia {
    background: url(<%= asset_path "dbpedia.png" %>) no-repeat 0 0;
}
.main section ul li.rss {
    background: url(<%= asset_path "rss.png" %>) no-repeat 0 0;
}
.main section ul li.tomita {
    background: url(<%= asset_path "tomita.png" %>) no-repeat 0 0;
}
.main section ul li.news {
    background: url(<%= asset_path "news.png" %>) no-repeat 0 0;
}
.main section ul li.api {
    background: url(<%= asset_path "api.png" %>) no-repeat 0 0;
}
.main section ul li.datahub {
    background: url(<%= asset_path "datahub.png" %>) no-repeat 0 0;
}
.main section ul li.github {
    background: url(<%= asset_path "github.png" %>) no-repeat 0 0;
}
.main section .item {
    font-size: 18px;
    padding: 3px 0;
}
.main section {
    display: inline-block; width: 49%; vertical-align: top;padding-bottom: 20px;
}
.main section .item span{
    color: #ff6000;
    font-size: 20px;
}
.qr {
    background: url(<%= asset_path "qr-code.gif" %>) no-repeat 0 0;
    width: 98px;
    height: 98px;
    position: absolute;
    right: 20px;
    bottom: 55px;
}